<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.graduate.laborManager.pub.bean.Staff" table="staff" schema="labormanager">
        <id name="id">
            <column name="id" sql-type="int(10) unsigned zerofill"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(20)" length="20" not-null="false"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(255)" not-null="false"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="int(11)" not-null="false"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(50)" length="50" not-null="false"/>
        </property>
        <property name="cId">
            <column name="c_id" sql-type="int(10)"/>
        </property>
        <property name="aId">
            <column name="a_id" sql-type="int(10)"/>
        </property>
    </class>
</hibernate-mapping>