<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.graduate.laborManager.pub.bean.Staff">
        <table name="staff" schema="labormanager" catalog=""/>
        <attributes>
            <id name="staffId">
                <column name="staff_id" length="32"/>
            </id>
            <basic name="name">
                <column name="name" nullable="false" length="20"/>
            </basic>
            <basic name="password">
                <column name="password" nullable="false"/>
            </basic>
            <basic name="phone">
                <column name="phone" nullable="false" length="11"/>
            </basic>
            <basic name="address">
                <column name="address" nullable="false" length="50"/>
            </basic>
            <basic name="companyId">
                <column name="company_id" nullable="false" length="32"/>
            </basic>
            <basic name="agreementId">
                <column name="agreement_id" nullable="false" length="32"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
