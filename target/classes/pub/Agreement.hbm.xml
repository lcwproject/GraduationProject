<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.graduate.laborManager.pub.bean.Agreement" table="agreement" schema="labormanager">
        <id name="id">
            <column name="id" sql-type="int(11) unsigned zerofill"/>
        </id>
        <property name="cId">
            <column name="c_id" sql-type="int(11)"/>
        </property>
        <property name="title">
            <column name="title" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="content">
            <column name="content" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>